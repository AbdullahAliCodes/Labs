#include <iostream>
#include <cmath>
#include <array>
using namespace std;

int main(){
    array<int, 10> marks;
    int pmark;
    int counter=0;
    int mark;
    for(int i; i<10; i++){
        cin >> mark;
        marks[i] =mark;
    }
    cin >> pmark;
    for(int element:marks){
        if(element>=pmark){
            counter++;
        }
    }
    cout << counter;
    return 0;
}
