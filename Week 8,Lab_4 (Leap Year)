#include <iostream>

using namespace std;
 
int y;
int main(){
    cin >> y;
    if ((y % 100 == 0) && (y % 400 == 0)){
        cout << y << "is a leap year" << endl;
    }
    else if (y % 100 != 0 && y % 4 == 0){
        cout << y << " " << "is a leap year" << endl;
    }
    else {
        cout << y << " " << "is not a leap year";
    }
    return 0;
}
    
